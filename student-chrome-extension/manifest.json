{
  "manifest_version": 3,
  "name": "Remote Browser Controller",
  "version": "0.1",
  "description": "Allows remote control of Chrome browser",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}